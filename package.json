{
  "name": "dudebot",
  "version": "1.0.0",
  "description": "An intelligent virtual assistant for workplace queries and task automation",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install-all": "npm install && cd server && npm install && cd client && npm install"
  },
  "keywords": ["ai", "assistant", "workplace", "automation", "chatbot"],
  "author": "DudeBot Team",
  "contributors": [
    "shreyaxgarg <https://github.com/shreyaxgarg>",
    "kkschauhan <https://github.com/kkschauhan>"
  ],
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
        "axios": "^1.5.0",

    "dotenv": "^16.3.1"
  },  
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}

